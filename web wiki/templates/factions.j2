{% extends "base.j2" %}


{% block content %}
<h2>Factions</h2>
<table>
    <tr><th></th><th>Name</th><th>Troop</th><th>Region</th></tr>
    {% for faction in factions %}
    <tr>
        <td><img src="{{faction["icon"]}}"></td>
        <td>{{faction["name"]}}</td>
        <td>{{faction["troop"]}}</td>
        <td>{{faction["region"]}}</td>
    </tr>
    {% endfor %}
{% endblock %}

